<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CameraView">
        <attr name="quality" format="enum">
            <enum name="very_high" value="0"/>
            <enum name="high" value="1"/>
            <enum name="medium" value="2"/>
            <enum name="low" value="3"/>
        </attr>
        <attr name="maxVideoDuration" format="integer" />
        <attr name="maxVideoSize" format="integer" />
    </declare-styleable>
    <declare-styleable name="VideoView">
        <attr name="filePath" format="string" />
    </declare-styleable>
</resources>